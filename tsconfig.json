{
  "compilerOptions": {
    "module": "esnext",
    "lib": ["dom", "esnext"],
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noUncheckedIndexedAccess": true,
    "noEmit": true
  },
  "exclude": ["node_modules"]
}
